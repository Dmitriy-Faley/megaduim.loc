        </div>
        <div class="swiper swiper-style products-new-swiper">
          <div class="swiper-wrapper">

          {% for product_man in products %}
              {% if product_man.attrblock %}
                {% for attrblockitem in product_man.attrblock %}
                    {% if attrblockitem.attribute_group_id == 32 %}
                        {% for attributeitem in attrblockitem.attribute %}
                            {% if attributeitem.name == 'Новинка' %}
                                      
                              <div class="swiper-slide">
                                <div class="card-product">
                                  <div class="card-product__head">
                                    <div class="card-product__image">

                                      <div class="hvr__images">
                                        <img class="hvr__image" src="{{ product_man.thumb }}" alt="{{ heading_title }}">
                                        {% for img in product_man.imgDop %}
                                          <img class="hvr__image" src="{{ img.thumb }}" alt="{{ heading_title }}">
                                        {% endfor %}
                                      </div>

                                    </div>
                                    <div class="card-product__content-mob">
                                      <div class="card-product__check">
                                        {% if product_man.stock == 'В наличии' %}
                                          <div class="card-product__status item1">В наличии</div>
                                          {% elseif product_man.stock == 'Нет в наличии' %}
                                          <div class="ard-product__status">Нет в наличии</div>
                                          {% elseif product_man.stock == 'Ожидание 2-3 дня' %}
                                          <div class="ard-product__status">Ожидание 2-3 дня</div>
                                          {% elseif product_man.stock == 'Предзаказ' %}
                                          <div class="ard-product__status">Предзаказ</div>
                                        {% endif %}
                                      </div>
                                      <div class="card-product__name1">{{ product_man.model }}</div>
                                      <a href="{{ product_man.href }}" class="card-product__name2">{{ product_man.name }}</a>
                                      <div class="card-product__articles">Арт. 5678909</div>
                                    </div>
                                  </div>
                                  <div class="card-product__body">
                                    <div class="card-product__content-desck">
                                      <div class="card-product__check">

                                      {% if product_man.stock == 'В наличии' %}
                                        <div class="card-product__status item1">В наличии</div>
                                        {% elseif product_man.stock == 'Нет в наличии' %}
                                        <div class="ard-product__status">Нет в наличии</div>
                                        {% elseif product_man.stock == 'Ожидание 2-3 дня' %}
                                        <div class="ard-product__status">Ожидание 2-3 дня</div>
                                        {% elseif product_man.stock == 'Предзаказ' %}
                                        <div class="ard-product__status">Предзаказ</div>
                                      {% endif %}
                                      
                                      </div>
                                      <div class="card-product__name1">{{ product_man.model }}</div>
                                      <a href="{{ product_man.href }}" class="card-product__name2">{{ product_man.name }}</a>
                                      <div class="card-product__articles">Арт. {{ product_man.sku }}</div>
                                    </div>
                                    <div class="card-product__line"></div>
                                    <div class="card-product__total">
                                      <div class="card-product__count">
                                        <button class="btn-reset card-product__count-button card-product__count-button-decrease">
                                          <svg width="12" height="3" viewBox="0 0 12 3" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M1 1.5H11" stroke="#919191" stroke-width="1.5" stroke-linecap="round" />
                                          </svg>
                                        </button>
                                        <input type="number" value="1" class="card-product__count-num">
                                        <button class="btn-reset card-product__count-button card-product__count-button-increase">
                                          <svg width="12" height="13" viewBox="0 0 12 13" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M1 6.5H11" stroke="#919191" stroke-width="1.5" stroke-linecap="round" />
                                            <path d="M6 1.5L6 11.5" stroke="#919191" stroke-width="1.5" stroke-linecap="round" />
                                          </svg>
                                        </button>
                                      </div>
                                      <div class="card-product__price-single"><span>{{ product_man.price }}</span> руб</div>
                                    </div>

                                    {% if product_man.stock == 'В наличии' %}
                                      <button
                                          class="hits_slide__btn add-to-cart {% if product_man.in_cart %} added{% endif %}"
                                          id="add-to-cart"
                                          onclick="add_in_cart('{{ product_man.product_id }}', '{{ product_man.minimum }}'); return false;">
                                          <img class="image image__empty"
                                              data-src="/image/catalog/assets/img/icon_cart_empty.svg" alt="" data-lazy="" />
                                          <img class="image image__active"
                                              data-src="/image/catalog/assets/img/icon_cart_active.svg" alt="" data-lazy="" />
                                      </button>
                                      {% endif %}

                                    <button class="btn-reset button2 card-product__button">В корзину</button>
                                    <div class="card-product__favourites">
                                      <div class="card-product__favourites-icon"></div>
                                      <span>Добавить в избранное</span>
                                    </div>
                                  </div>
                                </div>
                              </div>

                            {% endif %}
                          {% endfor %}
                      {% endif %}    
                  {% endfor %}
              {% endif %}
          {% endfor %}

