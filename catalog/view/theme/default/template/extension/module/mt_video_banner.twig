
{% if items %}
	<div class="mt-module mtvideo">	
		
        {% for item in items %}
			<div class="mtvideo-item">			
				<div class="mtvideo__img">		
					<div class="mtvideo_one">		
						<img src="{{ item.icon }}" loading="lazy" class="mtvideo__img-video" alt="{{ item.title }}" title="{{ item.title }}" width="100%" height="100%">
						<div class="mtvideo__img__time">{{ item.time }}</div>						
						<div class="mtvideo__img__play">
						<a href="{{ item.link }}" class="fancybox">
						<img width="100%" height="100%" src="data:image/svg+xml;base64," loading="lazy" alt="Воспроизвести" draggable="false" data-v-e4caeaf8="">
						</a>
						</div>
					</div>
					<div class="mtvideo__img__title">{{ item.title }}</div>
				</div>					
			</div>		
		{% endfor %}
		
	</div>
{% endif %}