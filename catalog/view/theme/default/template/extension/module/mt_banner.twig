  <div id="banner{{ module }}" class="moybb">
    {% for banner in banners %}
      <div class="moybb-item">{% if banner.link %}<a href="{{ banner.link }}"><img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive" loading="lazy" width="100%" height="100%" /></a>{% else %}<img src="{{ banner.image }}" loading="lazy" alt="{{ banner.title }}" class="img-responsive" width="100%" height="100%"/>{% endif %}</div>
      {% endfor %}
	  
  </div>

