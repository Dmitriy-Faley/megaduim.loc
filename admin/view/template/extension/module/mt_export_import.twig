<script type="text/javascript">
   window.onload = function() {
   {% if (export_open) %}
   window.open('{{ export_open }}'.replaceAll('&amp;', '&'), '_blank').focus();
   {% endif %}
   };
   
  $('#input-profile').change(function(){
    if (confirm('Сменить профиль? Несохраненные изменения сбросятся.')) {
      var url = '{{ current_link }}&profile=' + $(this).val() + '&profile_import=' + $('#input-profile-import').find(":selected").val();
      window.location.href = url.replaceAll('&amp;', '&');
    }
  });
  $('#input-profile-import').change(function(){
    if (confirm('Сменить профиль? Несохраненные изменения сбросятся.')) {
      var url = '{{ current_link }}&profile=' + $('#input-profile').find(":selected").val() + '&profile_import=' + $(this).val();
      window.location.href = url.replaceAll('&amp;', '&');
    }
  });
  $('#all_category').change(function() {
    if (this.checked) {
      $('#product-category').empty();
      $.ajax({
			  url: 'index.php?route=extension/module/mt_export_import/categories&user_token={{ user_token }}',
			  dataType: 'json',
			  success: function(json) {
          $.each(json, function(index, element) {
            $('#product-category').append('<div id="product-category' + element.category_id + '"><i class="fa fa-minus-circle"></i> ' + element.name + '<input type="hidden" name="" value="' + element.category_id + '" /></div>');
          });
			  },
        error: function(e) {
          alert(e.responseText);
        }
		  });
    }
    else {
      $('#product-category').empty();
    }
  })
  // Category
  $('input[name=\'category\']').autocomplete({
	  'source': function(request, response) {
		  $.ajax({
			  url: 'index.php?route=catalog/category/autocomplete&user_token={{ user_token }}&filter_name=' + encodeURIComponent(request),
			  dataType: 'json',
			  success: function(json) {
				  response($.map(json, function(item) {
					  return {
						  label: item['name'],
						  value: item['category_id']
					  }
				  }));
			  }
		  });
	  },
	  'select': function(item) {
		  $('input[name=\'category\']').val('');

		  $('#product-category' + item['value']).remove();

		  $('#product-category').append('<div id="product-category' + item['value'] + '"><i class="fa fa-minus-circle"></i> ' + item['label'] + '<input type="hidden" name="module_mt_export_import_category[]" value="' + item['value'] + '" /></div>');
	  }
  });
  $('#product-category').delegate('.fa-minus-circle', 'click', function() {
	  $(this).parent().remove();
  });
  // Manufacturer
  $('input[name=\'manufacturer\']').autocomplete({
	  'source': function(request, response) {
		  $.ajax({
			  url: 'index.php?route=catalog/manufacturer/autocomplete&user_token={{ user_token }}&filter_name=' + encodeURIComponent(request),
			  dataType: 'json',
